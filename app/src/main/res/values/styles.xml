<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary_material_light</item>
        <item name="colorPrimaryDark">@color/primary_dark_material_light_1</item>
        <item name="colorAccent">@color/accent_material_light</item>
        <item name="android:navigationBarColor">@color/primary_dark_material_light_1</item>
        <item name="android:textColorPrimary">@color/primary_text_material_light</item>
    </style>

    <style name="dark" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimaryDark">@color/primary_dark_material_light_1</item>
    </style>

    <style name="the_one" parent="android:Theme" >
        <item name="android:textColorPrimary">@color/primary_text_material_light</item>
    </style>

    <style name="theone" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="android:textColorPrimary">@color/primary_text_material_light_1</item>
        <item name="android:navigationBarColor">@android:color/background_light</item>
    </style>

</resources>
